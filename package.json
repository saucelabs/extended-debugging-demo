{
  "name": "extended_debugging_demo",
  "version": "0.0.1",
  "description": "Demo to show off extended debugging functionality",
  "main": "./node_modules/.bin/wdio",
  "scripts": {
    "build": "run-s build:*",
    "build:bower": "cd public && bower install && cd ..",
    "build:babel": "babel server/ -d build/",
    "eslint": "eslint ./server ./test ./wdio.*",
    "start": "npm run build && node ./build",
    "test": "run-s eslint test:wdio",
    "test:wdio": "wdio ./test/js/wdio.conf.js",
    "watch": "npm run build -- --watch",
    "demo:analytics": "wdio ./test/js/wdio.conf.js --suite analytics",
    "demo:offline": "wdio ./test/js/wdio.conf.js --suite offline",
    "demo:blocked": "wdio ./test/js/wdio.conf.js --suite blocked",
    "demo:pageload": "wdio ./test/js/wdio.conf.js --suite pageload",
    "demo:populate": "wdio ./test/js/wdio.conf.js --suite populate",
    "demo:prepopulate": "wdio ./test/js/wdio.conf.js --suite prePopulate",
    "demo:debug": "wdio ./test/js/wdio.conf.js --suite debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christian-bromann/extended_debugging_demo.git"
  },
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/christian-bromann/extended_debugging_demo/issues"
  },
  "homepage": "https://github.com/christian-bromann/extended_debugging_demo#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@wdio/cli": "^5.10.9",
    "@wdio/local-runner": "^5.10.9",
    "@wdio/mocha-framework": "^5.10.8",
    "@wdio/sauce-service": "^5.10.8",
    "@wdio/spec-reporter": "^5.9.3",
    "@wdio/sync": "^5.10.8",
    "bootstrap": "^3.4.0",
    "bower": "^1.8.8",
    "cors": "^2.8.4",
    "deepmerge": "^1.5.2",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "express": "^4.16.1",
    "npm-run-all": "^4.1.2",
    "webdriver": "^5.10.9",
    "webdriverio": "^5.10.9",
    "yargs": "^11.0.0"
  }
}
